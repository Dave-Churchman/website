  <div class="grid-container">
    <%= partial "partials/nav" %>
  </div>
</header>

<main class="main-container" role="main">
  <section class="home-hero">
    <div class="grid-container">
      <h1>FAQs</h1>
        <!-- <ul> -->
          <% data.faq.each do |f| %>
            <h3><%= f["question"] %></h3>
            <p><%= f["answer"] %></p>
          <% end %>
        <!-- </ul> -->
